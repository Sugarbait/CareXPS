{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/dashboard",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/calls",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/sms",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/settings",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/login",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/manifest.webmanifest", "/sw.js", "/registerSW.js", "/workbox-*.js", "/*.{png,jpg,gif,svg,ico,css,js,json,woff,woff2,ttf}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon",
    ".woff2": "font/woff2",
    ".woff": "font/woff",
    ".ttf": "font/ttf",
    ".eot": "application/vnd.ms-fontobject",
    ".webmanifest": "application/manifest+json"
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://login.microsoftonline.com https://api.retellai.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://login.microsoftonline.com https://api.retellai.com https://*.supabase.co wss://*.supabase.co; frame-src 'self' https://login.microsoftonline.com;"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "forwardingGateway": {
    "allowedForwardedHosts": ["carexps.nexasync.ca"]
  },
  "trailingSlash": "auto"
}
